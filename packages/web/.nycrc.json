{
  "extension": [".js", ".jsx", ".ts", ".tsx"],
  "extends": "@istanbuljs/nyc-config-typescript",
  "include": ["pages_/**/*"],
  "exclude": ["pages/api/__coverage__.js"],
  "report-dir": "cypress-coverage",
  "reporter": [
    "json",
    "lcov"
  ],
  "all": true,
  "sourceMap": true,
  "instrument": true
}
